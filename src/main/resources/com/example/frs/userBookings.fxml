<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="566.0" prefWidth="1006.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.frs.UserBookingsController">
   <children>
      <ImageView fitHeight="592.0" fitWidth="1006.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/bg/skyimg.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="passengerInfo" layoutX="42.0" layoutY="53.0" prefHeight="364.0" prefWidth="923.0">
        <columns>
          <TableColumn fx:id="bookingId" prefWidth="91.0" text="Booking Id" />
          <TableColumn fx:id="flightId" prefWidth="105.0" text="Flight Id" />
            <TableColumn fx:id="from" prefWidth="119.0" text="From" />
            <TableColumn fx:id="to" prefWidth="118.0" text="To" />
            <TableColumn fx:id="passengerName" prefWidth="236.0" text="Pasenger Name" />
            <TableColumn fx:id="departureDate" prefWidth="125.0" text="Departure Date" />
            <TableColumn fx:id="departureTime" prefWidth="128.0" text="Departure Time" />
        </columns>
      </TableView>
      <Button fx:id="cancel_btn" layoutX="42.0" layoutY="450.0" mnemonicParsing="false" onAction="#cancelFlight" prefHeight="18.0" prefWidth="156.0" style="-fx-background-color: #64B5F6;" text="Cancel Flight" textFill="WHITE">
         <font>
            <Font name="Microsoft Sans Serif" size="21.0" />
         </font>
      </Button>
      <Button fx:id="back_btn" layoutX="254.0" layoutY="450.0" mnemonicParsing="false" onAction="#back" prefHeight="39.0" prefWidth="140.0" style="-fx-background-color: #64B5F6;" text="Back" textFill="WHITE">
         <font>
            <Font name="Microsoft Sans Serif" size="21.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
